{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": true,
    "module": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "allowJs": true,
    "jsx": "react"
  },
  "include": ["./src/**/*", "typings.d.ts"],
  "exclude": ["node_modules", "importlater"]
}
